    <!-- urn:draid:core#Acknowledge -->

    <owl:Class rdf:about="urn:draid:core#Acknowledge">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Obligation"/>
        <rdfs:comment>The user should properly acknowledge the data source, using the statement contained in the argument.
The argument should be a string representing the expected form of acknowledgement.</rdfs:comment>
    </owl:Class>
    


    <!-- urn:draid:core#Cite -->

    <owl:Class rdf:about="urn:draid:core#Cite">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Obligation"/>
        <rdfs:comment>The user should properly give citation to the given source, represented in the argument.
The argument should be the citation target, either as text (string) in an appropriate citation style, or a URL to a webpage containing the appropriate text.</rdfs:comment>
    </owl:Class>
    


    <!-- urn:draid:core#Finished -->

    <owl:Class rdf:about="urn:draid:core#Finished">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Stage"/>
        <owl:disjointWith rdf:resource="urn:draid:core#Imported"/>
    </owl:Class>
    


    <!-- urn:draid:core#Imported -->

    <owl:Class rdf:about="urn:draid:core#Imported">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Stage"/>
    </owl:Class>
    


    <!-- urn:draid:core#Include -->

    <owl:Class rdf:about="urn:draid:core#Include">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Obligation"/>
        <rdfs:comment>User should read the precise policy represented in the argument.
It should normally be an URL to a single policy text, or the policy text itself.</rdfs:comment>
    </owl:Class>
    


    <!-- urn:draid:core#Obligation -->

    <owl:Class rdf:about="urn:draid:core#Obligation">
        <rdfs:comment>Obligated action class</rdfs:comment>
    </owl:Class>
    


    <!-- urn:draid:core#Processing -->

    <owl:Class rdf:about="urn:draid:core#Processing">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Stage"/>
    </owl:Class>
    


    <!-- urn:draid:core#Report -->

    <owl:Class rdf:about="urn:draid:core#Report">
        <rdfs:subClassOf rdf:resource="urn:draid:core#Obligation"/>
        <rdfs:comment>User should report their usage of the data to the location represented in the argument.
The argument should be a URL.</rdfs:comment>
    </owl:Class>
    


    <!-- urn:draid:core#Stage -->

    <owl:Class rdf:about="urn:draid:core#Stage"/>
